CLIENT_ID=your_client_id_here
CLIENT_SECRET=your_client_secret_here

# Telegram Bot Token
# Получите у @BotFather и вставьте сюда
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Пример:
# CLIENT_ID=abc123def456ghi789
